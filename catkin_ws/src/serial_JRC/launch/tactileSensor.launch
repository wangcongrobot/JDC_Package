<launch>	
	<node pkg="serial" type="serial_example" name="tactile_com_sensor" output="screen">
	<param name="dev_name" type="string" value="/dev/ttyUSB0" />
	<param name="data_record" type="bool" value="false" />
	<param name="filePath" type="string" value="/home/robot/SensorRecord.txt" />
	<param name="fifo_send_on" type="bool" value="true" />	
	<param name="old_vesion_support" type="bool" value="true" />	
	</node>		
</launch>
